{% extends 'base.html.twig' %}

{% block title %}Domicile index{% endblock %}

{% block body %}
    <h1>Domicile index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for domicile in domiciles %}
            <tr>
                <td>{{ domicile.id }}</td>
                <td>
                    <a href="{{ path('app_domicile_show', {'id': domicile.id}) }}">show</a>
                    <a href="{{ path('app_domicile_edit', {'id': domicile.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_domicile_new') }}">Create new</a>
{% endblock %}
